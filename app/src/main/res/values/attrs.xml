<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ShapeImageViewStyle">
        <attr name="image_border_width" format="dimension" />
        <attr name="image_border_color" format="color" />
        <attr name="image_radius" format="dimension" />
        <attr name="image_shape_type" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="circle" value="1" />
            <enum name="super_oval" value="2" />
        </attr>
    </declare-styleable>

</resources>